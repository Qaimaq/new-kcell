        <!-- CONTAINER -->
        <div class="container" data-appear-top-offset="-200" data-animated="fadeInUp">
            <div class="row">
                <!-- CASTOM TAB -->
                <div class="col-md-10 col-lg-10">
                    <div id="myTabContent" class="tab-content">
                        <div class="tab-pane fade in active clearfix" id="tab1">
                            <p class="title"><b>@Localizer["More_about_us"]</b></p>
                            <span style="color: #fff;text-shadow: 0 0 20px #000;">@Localizer["Slogan"]</span>


                        </div>
                    </div>
                </div>
                <div class="col-md-2 col-lg-2">
                    <div class="text-center">
                        <a asp-area="" asp-action="About" asp-route-culture="@culture">
                            <div class="btn-theme btn-theme-sm btn-white-brd text-uppercase wow fadeInDown animated text-center" data-animation="fadeInDown" data-wow-duration="1000ms" data-wow-delay="1500ms" style=" ">
                                @Localizer["About"]
                            </div>
                        </a>
                    </div>
                </div>
            </div>
        </div><!-- //CONTAINER -->
    </div><!-- //CLEAN CODE -->
</section>
