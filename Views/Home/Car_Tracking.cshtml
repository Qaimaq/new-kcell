
@{
    ViewData["Title"] = @Localizer["Car_Tracking"];
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Mvc.Localization
@using Microsoft.AspNetCore.Builder
@using Microsoft.AspNetCore.Http.Features
@using Microsoft.AspNetCore.Localization
@using Microsoft.Extensions.Options
@using Microsoft.Extensions.Localization
@inject IOptions<RequestLocalizationOptions> LocOptions

@inject IViewLocalizer Localizer

@inject IHtmlLocalizer<SharedResource> SharedLocalizer
@{
    var culture = Context.Features.Get<IRequestCultureFeature>
        ().RequestCulture.Culture.TwoLetterISOLanguageName;
}

@using Microsoft.AspNetCore.Routing.Constraints;
<!-- BREADCRUMBS -->
<section class="breadcrumbs_block breadcrumbs_block_car_tracking clearfix parallax">
    <div class="container center">
        <h2 class="marginleft85" style="text-align:left; margin-left: 120px;"><b>@Localizer["Car_Tracking"]</b></h2>
        <p class="marginleft85" style="color: #fff;text-shadow: 0 0 20px #000; text-align:left; margin-left: 120px;">@Localizer["Slogan"]</p>
    </div>
</section><!-- //BREADCRUMBS -->
<!-- start about Section -->
<section id="abt_sec">
    <div class="container">
        <div class="row">
            <!--
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
        <div class="title_sec">
            <h1>@Localizer["Car_Tracking"]</h1>
            <hr style="margin-top:0;" />
            <h2>@Localizer["Slogan"]</h2>
        </div>
    </div>
        -->
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
                <div class="abt">
                    <p class="desc-text" style="margin-bottom:30px">
                        @Localizer["Text1"] @Localizer["List1"] @Localizer["List2"] @Localizer["List3"] @Localizer["List4"] 
