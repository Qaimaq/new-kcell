<section class="breadcrumbs_block breadcrumbs_block_sms_dispatching clearfix parallax">
    <div class="container center">
        <h2 class="marginleft85" style="text-shadow: 0 0 20px #000; text-align:left; margin-left: 120px;"><b>@Localizer["SMS_Dispatching"]</b></h2>
        <p class="marginleft85" style="color: #fff;text-shadow: 0 0 20px #000; text-align:left; margin-left: 120px;">@Localizer["Slogan"]</p>
    </div>
</section><!-- //BREADCRUMBS -->
<!-- start about Section -->
<section id="abt_sec" style="padding: 30px 0;">
