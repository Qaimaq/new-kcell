                        <p class="desc-text" style="text-align:left; margin-bottom:20px">@Localizer["Text1"] @Localizer["Text2"] @Localizer["Text3"] @Localizer["Text4"]</p>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs6 ">
                        <h4 style="margin-bottom: 20px; text-align:left; color: #888"><b>@Localizer["SMS_Title"]</b></h4>
                        <ul style="text-align:left;">
                        <li style="list-style-type:square">@Localizer["SMS_text1"]</li>
                        <li style="list-style-type:square">@Localizer["SMS_text2"]</li>
                        <li style="list-style-type:square">@Localizer["SMS_text3"]</li>
                        <li style="list-style-type:square">@Localizer["SMS_text4"]</li>
                        </ul>
                    </div>
                  </div>
            </div>
        </div>
    </div>
</section>
