
@{
    ViewData["Title"] = @Localizer["Software"];
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Mvc.Localization

@inject IViewLocalizer Localizer

@inject IHtmlLocalizer<SharedResource> SharedLocalizer
<!-- BREADCRUMBS -->
<section class="breadcrumbs_block clearfix parallax">
    <div class="container center">
        <h2><b>@Localizer["Software"]</b></h2>
        <p>@Localizer["Slogan"]</p>
    </div>
</section><!-- //BREADCRUMBS -->
<!-- start about Section -->
<section id="abt_sec">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
                <div class="title_sec">
                    <h1>@Localizer["Software"]</h1>
                    <h2>@Localizer["Slogan"]</h2>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
                <div class="abt">
                    <p>
                        @Localizer["Text1"]<br />
                        @Localizer["Text2"]<br />
                        @Localizer["Text3"]
                    </p>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- End About Section -->
<!-- ABOUT -->
<section id="about">
    <!-- MULTI PURPOSE -->
    <div class="purpose_block">
        <!-- CONTAINER -->
        <div class="container">
            <!-- ROW -->
            <div class="row">
                <div class="row margbot50">
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="col-lg-12 col-md-12 col-sm-12" data-appear-top-offset="-200" data-animated="fadeInLeft">
                            <h2><b>@Localizer["Our"]</b> @Localizer["office"]</h2>
                            <p>@Localizer["Our_office_text1"]</p>
                            <p>@Localizer["Our_office_text2"]</p>

                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12" data-appear-top-offset="-200" data-animated="fadeInLeft">
                            <h2><b>@Localizer["Ours"]</b> @Localizer["advantages"]</h2>
                            <p>@Localizer["Ours_advantages_text1"]</p>
                            <p>@Localizer["Ours_advantages_text2"]</p>

                        </div>
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6" data-appear-top-offset="-200" data-animated="fadeInDown">
                        <img class="img-responsive" src="~/images/people.jpg" alt="Alternate Text" />
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6" data-appear-top-offset="-200" data-animated="fadeInLeft">
                    <h2><b>@Localizer["Cloud"]</b> @Localizer["programs"]</h2>
                    <p>@Localizer["Cloud_programs_text1"]</p>
                    <p>@Localizer["Cloud_programs_text2"]</p>

                </div>

                <div class="col-lg-6 col-md-6 col-sm-6" data-appear-top-offset="-200" data-animated="fadeInLeft">
                    <h2><b>@Localizer["Free"]</b> @Localizer["consultation"]</h2>
                    <p>@Localizer["Free_consultation_text1"]</p>
                    <p>@Localizer["Free_consultation_text2"]</p>
                    <p>@Localizer["Free_consultation_text3"]</p>

                </div>

            </div><!-- //ROW -->
        </div><!-- //CONTAINER -->
    </div><!-- //MULTI PURPOSE -->
</section><!-- //ABOUT -->
