    </section><!-- //PROJECTS -->
    <!-- CLEAN CODE -->
    <div class="cleancode_block">
        <!-- CONTAINER -->
        <div class="container" data-appear-top-offset="-200" data-animated="fadeInUp">
            <!-- CASTOM TAB -->
            <div id="myTabContent" class="tab-content">
                <div class="tab-pane fade in active clearfix" id="tab1">
                    <p class="title"><b>@Localizer["Tab1_text1"]</b> @Localizer["Tab1_text2"]</p>
                    <span style="color: #fff;">@Localizer["Tab1_text3"]</span>
                </div>
                <div class="tab-pane fade clearfix" id="tab2">
                    <p class="title"><b>@Localizer["Tab2_text1"]</b> @Localizer["Tab2_text2"]</p>
                    <span style="color: #fff;">@Localizer["Tab2_text3"]</span>
                </div>
                <div class="tab-pane fade clearfix" id="tab3">
                    <p class="title"><b>@Localizer["Tab3_text1"]</b> @Localizer["Tab3_text2"]</p>
                    <span style="color: #fff;">@Localizer["Tab3_text3"]</span>
                </div>
                <div class="tab-pane fade clearfix" id="tab4">
                    <p class="title"><b>@Localizer["Tab4_text1"]</b> @Localizer["Tab4_text2"]</p>
                    <span style="color: #fff;">@Localizer["Tab4_text3"]</span>
                </div>
                <div class="tab-pane fade clearfix" id="tab5">
                    <p class="title"><b>@Localizer["Tab5_text1"]</b> @Localizer["Tab5_text2"]</p>
                    <span style="color: #fff;">@Localizer["Tab5_text3"]</span>
                </div>
            </div>
            <ul id="myTab" class="nav nav-tabs">
                <li class="active"><a href="#tab1" data-toggle="tab"><i class="fa fa-cog fa-3x service-icon-tabs"></i><span>@Localizer["Tab1_text4"]</span></a></li>
                <li><a href="#tab2" data-toggle="tab"><i class="fa fa-wrench fa-3x service-icon-tabs"></i><span>@Localizer["Tab2_text4"]</span></a></li>
                <li><a href="#tab3" data-toggle="tab"><i class="fa fa-desktop fa-3x service-icon-tabs"></i><span>@Localizer["Tab3_text4"]</span></a></li>
                <li><a href="#tab4" data-toggle="tab"><i class="fa fa-file fa-3x service-icon-tabs"></i><span>@Localizer["Tab4_text4"]</span></a></li>
                <li><a href="#tab5" data-toggle="tab"><i class="fa fa-star fa-3x service-icon-tabs"></i><span>@Localizer["Tab5_text4"]</span></a></li>
            </ul><!-- CASTOM TAB -->
        </div><!-- //CONTAINER -->
    </div><!-- //CLEAN CODE -->

</section><!-- //ABOUT -->
