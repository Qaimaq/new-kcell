@{
    ViewData["Title"] = "Kсell Solutions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Builder
@using Microsoft.AspNetCore.Http.Features
@using Microsoft.AspNetCore.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@using Microsoft.Extensions.Options
@using Microsoft.Extensions.Localization
@inject IHtmlLocalizer<SharedResource> SharedLocalizer
@inject IOptions<RequestLocalizationOptions> LocOptions
@using Microsoft.AspNetCore.Mvc.Localization

@{
    var culture = Context.Features.Get<IRequestCultureFeature>
        ().RequestCulture.Culture.TwoLetterISOLanguageName;
}

@using Microsoft.AspNetCore.Routing.Constraints;

@inject IViewLocalizer Localizer

<!-- HOME -->
<section id="home" class="padbot0">
    <!-- TOP SLIDER -->
    <div class="flexslider top_slider">
        <ul class="slides">
            <li class="slide1">
                <div class="flex_caption1">
                    <p class="title1 captionDelay2 FromTop">Kcell </p>
                    <p class="title2 captionDelay4 FromTop">Solutions </p>
                    <p class="title4 captionDelay8 FromBottom">@Localizer["Slogan"]</p>
                </div>
                <a class="slide_btn FromRight" asp-area="" asp-action="About" asp-route-culture="@culture">@Localizer["More"]</a>
            <li class="slide2">
                <div class="flex_caption1">
                    <p class="title1 captionDelay2 FromTop">Kcell </p>
                    <p class="title2 captionDelay4 FromTop">Solutions </p>
                    <p class="title4 captionDelay8 FromLeft">@Localizer["Slogan2"]</p>
                </div>
                <a class="slide_btn FromRight" asp-area="" asp-action="Contacts" asp-route-culture="@culture">@Localizer["Contacts"]</a>
            </li>
        </ul>
        <!-- VIDEO BACKGROUND -->
        <a name="P2" class="player" id="P2" data-property="{videoURL:'HDOjxwjqCxI',containment:'.top_slider',autoPlay:true, mute:true, startAt:0, opacity:1}"></a>
        <!-- //VIDEO BACKGROUND -->
    </div><!-- //TOP SLIDER -->
</section><!-- //HOME -->
<!-- ABOUT -->
<section id="about">
    <div class="row row-space-1 margin-b-2 wow fadeInDown animated" style="visibility:visible; animation-name: fadeInDown;">
        <h3 class="" style="text-align: center; color: #652E84;">@Localizer["Welcome"]</h3>
        <hr style="margin-top:0;" />
        <div class="col-sm-10 col-sm-offset-1">
            <p class="margin-b-40 text-center wow fadeInDown animated fweight-400 title-desc" style="visibility:visible; animation-name: fadeInDown;text-align:center;font-size:16px;">
                @Localizer["Text1"] @Localizer["Text2"]
            </p>
        </div>
    </div>
</section><!-- //ABOUT -->
<!-- ABOUT -->
<section id="about">
    <!-- SERVICES -->
    <div class="services_block padbot40" data-appear-top-offset="-200" data-animated="fadeInUp">
        <!-- CONTAINER -->
        <div class="container">
            <!-- ROW -->
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-6 col-xs-6 col-ss-12 margbot30" style="text-align: center;">
                    <a asp-area="" asp-action="Software" asp-route-culture="@culture">
                        <h3 style="color: #888">@Localizer["Software"]</h3><i class="fa fa-laptop fa-3x service-icon-home home-icon"></i>
                        <p class="desc-text" style="margin: 0;padding: 0;padding-bottom: 10px!important; text-align:left;">@Localizer["Software_text"] @Localizer["Software_text2"] @Localizer["Software_text3"]</p>
                    </a>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-6 col-xs-6 col-ss-12 margbot30" style="text-align: center;">
                    <a asp-area="" asp-action="Car_Tracking" asp-route-culture="@culture">
                        <h3 style="color: #888">@Localizer["Car_Tracking"]</h3><i class="fa fa-search fa-3x service-icon-home home-icon"></i>
                        <p class="desc-text" style="margin: 0;padding: 0;padding-bottom: 10px!important; text-align:left;">@Localizer["Car_Tracking_text"] @Localizer["Car_Tracking_text2"] @Localizer["Car_Tracking_text3"]</p>
                    </a>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-6 col-xs-6 col-ss-12 margbot30" style="text-align: center;">
                    <a asp-area="" asp-action="SMS_Dispatching" asp-route-culture="@culture">
                        <h3 style="color: #888">@Localizer["SMS_Dispatching"]</h3><i class="fa fa-envelope fa-3x service-icon-home home-icon"></i>
                        <p class="desc-text" style="margin: 0;padding: 0;padding-bottom: 10px!important; text-align:left;">@Localizer["SMS_Dispatching_text"] @Localizer["SMS_Dispatching_text2"] @Localizer["SMS_Dispatching_text3"]</p>
                    </a>
                </div>
                
            </div><!-- //ROW -->
        </div><!-- //CONTAINER -->
    </div><!-- //SERVICES -->
    <!-- PROJECTS -->
